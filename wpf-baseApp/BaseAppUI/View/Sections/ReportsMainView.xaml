                            <TextBlock x:Name="A1textBlockMTDSalesValue" HorizontalAlignment="Left" Grid.Row="0"  Text="{Binding MTDsales}" VerticalAlignment="Top" FontSize="64" Foreground="#FFF5E6E6" IsEnabled="False"/>

